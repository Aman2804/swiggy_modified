<% @items.each do |item| %>
	<%= item.name %>
	<%= item.category %>
	<%= item.type_of_dish %>
		<%= link_to 'edit', edit_item_path(item) %>
		<%= link_to 'Delete', item_path(item), method: :delete, data: { confirm:
          'Are you sure?' } %>
	<br />
<% end %>
<%= link_to 'Add Item', new_item_path %>