<%= form_for @restaurant do |restaurant|%>
	<%= restaurant.label :Restaurant_Name %>
	<%= restaurant.text_field :name %><br />
	<%= restaurant.label :designation%>
	<%= restaurant.select(:designation, options_for_select([['Owner', 'owner'], ['Manager', 'manager']],'owner')) %><br />
	<%= restaurant.submit "submit"%>
<% end %>