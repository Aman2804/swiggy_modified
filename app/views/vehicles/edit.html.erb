<div class="container" style="margin-left: 459px">
  <h3 style="color: #614a4e;margin-left: 17px">Vehicle</h3>
  <div class="col-sm-3 col-md-6 col-lg-4" style="color:black">
    <%= form_for @vehicle do |form| %>
      <div class="form-group">
        <%= form.label :vehicle %><br>
        <%= form.select(:vehicle, options_for_select([['Bike', 'bike'], ['Bicycle', 'bicycle']],'bike'), {}, {class: "btn btn-primary dropdown-toggle ", style: 'background-color:#333'})%>
      </div>

      <div class="form-group">
        <%= form.label "Vehicle Name " %>
        <%= form.text_field :vehicle_name,class: "form-control" %>
      </div>

      <div class="actions">
        <%= form.submit "Modify",class: "btn btn-primary", style: 'width:-webkit-fill-available;background-color:#333' %>
      </div>
    <% end %>
  </div>
</div>
